<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        <bean id="emp1" class="com.Employee">
        <property name="empId" value = "101"></property>
        <property name="empName" value = "Alice"></property>
        <property name="salary"  value = "50000.0"></property>
        </bean>
    
        <bean id="emp2" class="com.Employee">
        <constructor-arg value = "102"></constructor-arg>
        <constructor-arg value ="Bob"></constructor-arg>
        <constructor-arg value = "25000.0"></constructor-arg>
        </bean>
        

</beans>